{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        // Possible Errors
        "no-console": 0,
        // Node.js and CommonJS
        "callback-return": 0,
        "global-require": 0,
        "handle-callback-err": 2,
        "no-mixed-requires": 2,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-restricted-modules": 0,
        "no-sync": 0,
        "no-underscore-dangle": 0,
        "consistent-return": 0,
        "import/no-extraneous-dependencies": 0
    }
}
